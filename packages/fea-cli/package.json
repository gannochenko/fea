{
    "name": "@gannochenko/fea-cli",
    "version": "0.0.0",
    "license": "MIT",
    "files": [
        "build.esm/",
        "build.cjs/"
    ],
    "bin": {
        "fea": "./build.cjs/index.js"
    },
    "main": "build.cjs/index.js",
    "module": "build.esm/index.js",
    "typings": "build.cjs/index.d.ts",
    "scripts": {
        "dev": "DEBUG=* ts-node src/index.ts",
        "debug": "node --inspect-brk -r ts-node/register src/index.ts",
        "link-global": "./script/link.sh",
        "build": "rm -rf ./build/* && yarn run build:esm && yarn run build:cjs",
        "build:esm": "del ./build.esm/* && tsc -p ./tsconfig.build.esm.json",
        "build:cjs": "del ./build.cjs/* && tsc -p ./tsconfig.build.cjs.json",
        "build:watch": "tsc -w -p ./tsconfig.build.cjs.json",
        "build:dev": "yarn build:watch",
        "install-peers": "install-self-peers -- --ignore-scripts",
        "test": "jest -c ./jest/config.js --passWithNoTests",
        "test:watch": "jest -c ./jest/config.js --watch",
        "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
        "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
        "release": "./script/release.sh"
    },
    "dependencies": {
        "case-formatter": "^1.1.2",
        "chalk": "^4.1.2",
        "clear": "^0.1.0",
        "commander": "^8.1.0",
        "debug": "^4.3.2",
        "execa": "^5.1.1",
        "figlet": "^1.5.2",
        "find-up-all": "^1.0.2",
        "fs-extra": "^10.0.0",
        "inquirer": "^8.1.2"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:gannochenko/fea-cli.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/git": "^9.0.1",
        "@semantic-release/npm": "^7.1.3",
        "@semantic-release/release-notes-generator": "^9.0.3",
        "@team-griffin/install-self-peers": "^1.1.1",
        "@types/clear": "^0.1.2",
        "@types/debug": "^4.1.7",
        "@types/figlet": "^1.5.4",
        "@types/fs-extra": "^9.0.12",
        "@types/inquirer": "^8.1.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.9.1",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "del-cli": "^4.0.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.1.1",
        "jest-chain": "^1.1.5",
        "pkg": "^5.3.1",
        "semantic-release": "^17.4.7",
        "ts-essentials": "^8.1.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.2"
    }
}
